@base <https://mcal.odissei.nl/cv/contentFeature/v0.1/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .


<> a skos:ConceptScheme ;
    skos:prefLabel "MCAL Controlled Vocabulary for Content Features"@en ;
    dct:description "Descriptors for different features used in content type analysis. "@en ;
    dct:licence <https://creativecommons.org/licenses/by-sa/4.0/> ;
    dct:title "MCAL Content Features"@en ;
    owl:versionInfo "0.1" ;
    skos:notation "contentFeature" ;
    dct:isVersionOf <https://mcal.odissei.nl/cv/contentFeature/> ;
    dct:creator "Angelica Maineri" ;
    dct:creator "Jacco van Ossenbruggen" ;
    dct:creator "Susan Vermeer" ;
    dct:contributor "Anne Kroon" ;
    dct:contributor "Rens Vliegenthart" ;
    skos:hasTopConcept <CFR1>, <CFE5>.


<CF0> a skos:Concept;
    skos:prefLabel "Other"@en ;
    skos:scopeNote "Unspecified or missing content analysis type, usage should be avoided as much as possible"@en ;
    skos:inscheme <> .
    
<CFR1> a skos:Concept;
    skos:prefLabel "Framing"@en ;
    skos:narrower <CAT5>, <CAT6> ;
    skos:topConceptOf <> ;
    skos:inscheme <> .

<CFR4> a skos:Concept;
    skos:prefLabel "Associative framing"@en ;
    skos:broader <CFR1> ;
    skos:inscheme <> .

<CFR5> a skos:Concept;
    skos:prefLabel "Symbolic framing"@en ;
    skos:broader <CFR1> ;
    skos:inscheme <> .

<CFE5> a skos:Concept;
    skos:prefLabel "Concept visibility"@en ;
    skos:topConceptOf <> ;
    skos:inscheme <> .


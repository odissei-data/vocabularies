  dct:identifier "f23a3803-52c2-4d7b-a6a4-c9f0fbbec0f4-nl-nl";
  owl:versionInfo 1 .

<https://vocabs.cbs.nl/def/begrippen/1833> a skos:Concept;
  skos:prefLabel "Brutotonnage"@nl, "Gross tonnage"@en;

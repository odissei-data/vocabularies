  skos:related <https://vocabs.cbs.nl/def/begrippen/212>, <https://vocabs.cbs.nl/def/begrippen/1882>,
    <https://vocabs.cbs.nl/def/begrippen/1612>;
  skos:historyNote "Van het CBS is uit de definitie gelaten."@nl;
  dct:identifier "26dc929b-57da-4002-81e9-f212f72e4fd7-nl-nl";
  owl:versionInfo 2 .

